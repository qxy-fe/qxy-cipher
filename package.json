{
  "name": "@qxy/cipher",
  "version": "0.0.8",
  "packageManager": "pnpm@8.6.2",
  "description": "QXY cipher utils",
  "keywords": [
    "qxy",
    "cipher",
    "crypto-js"
  ],
  "license": "MIT",
  "author": {
    "name": "ntnyq",
    "email": "ntnyq13@gmail.com"
  },
  "homepage": "https://github.com/qxy-fe/qxy-cipher#readme",
  "bugs": {
    "url": "https://github.com/qxy-fe/qxy-cipher/issues"
  },
  "repository": "qxy-fe/qxy-cipher",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./*": "./*"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "unbuild",
    "coverage": "vitest --coverage",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "prepublishOnly": "pnpm run build",
    "release": "run-s release:check release:version release:publish",
    "release:changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "release:check": "run-s typecheck lint",
    "release:publish": "pnpm -r publish",
    "release:version": "bumpp package.json -x \"pnpm release:changelog\" --all",
    "test": "vitest",
    "typecheck": "vitest typecheck"
  },
  "peerDependencies": {
    "crypto-js": "^4.1.1"
  },
  "dependencies": {
    "@types/crypto-js": "^4.1.1",
    "crypto-js": "^4.1.1"
  },
  "devDependencies": {
    "@ntnyq/eslint-config": "^1.9.0",
    "@ntnyq/prettier-config": "^1.9.0",
    "@types/node": "^20.3.1",
    "@vitest/coverage-v8": "^0.32.0",
    "bumpp": "^9.1.1",
    "conventional-changelog-cli": "^3.0.0",
    "eslint": "^8.42.0",
    "npm-run-all": "^4.1.5",
    "pnpm": "^8.6.2",
    "prettier": "^2.8.8",
    "typescript": "^5.1.3",
    "unbuild": "^1.2.1",
    "vitest": "^0.32.0"
  },
  "prettier": "@ntnyq/prettier-config",
  "publishConfig": {
    "access": "public"
  }
}
