{
  "name": "@qxy/cipher",
  "version": "0.0.3",
  "description": "QXY cipher utils",
  "keywords": [
    "qxy",
    "cipher",
    "crypto-js"
  ],
  "homepage": "https://github.com/qxy-fe/qxy-cipher#readme",
  "bugs": {
    "url": "https://github.com/qxy-fe/qxy-cipher/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qxy-fe/qxy-cipher.git"
  },
  "license": "MIT",
  "author": {
    "name": "ntnyq",
    "email": "ntnyq13@gmail.com"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rimraf dist *.tsbuildinfo",
    "build": "run-s clean build:lib",
    "build:lib": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint --ext .js,.ts .",
    "prepublishOnly": "pnpm run build",
    "release": "run-s release:check release:version release:publish",
    "release:check": "run-s typecheck lint clean build",
    "release:publish": "pnpm -r publish",
    "release:changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "release:version": "bumpp package.json --execute=\"pnpm release:changelog\" --commit \"build: release v%s\" --all --tag --push",
    "test": "vitest --passWithNoTests",
    "coverage": "vitest --coverage",
    "typecheck": "tsc --noEmit"
  },
  "eslintConfig": {
    "extends": "@qxy/typescript",
    "root": true
  },
  "dependencies": {
    "@types/crypto-js": "^4.1.1",
    "crypto-js": "^4.1.1"
  },
  "peerDependencies": {
    "crypto-js": "^4.1.1"
  },
  "devDependencies": {
    "@qxy/eslint-config-typescript": "^0.1.2",
    "@types/node": "^17.0.31",
    "bumpp": "^7.1.1",
    "c8": "^7.11.2",
    "conventional-changelog-cli": "^2.2.2",
    "eslint": "^8.14.0",
    "npm-run-all": "^4.1.5",
    "pnpm": "^7.0.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.6.4",
    "unbuild": "^0.7.4",
    "vite": "^2.9.8",
    "vitest": "^0.10.5"
  },
  "packageManager": "pnpm@7.0.0",
  "publishConfig": {
    "access": "public"
  }
}
