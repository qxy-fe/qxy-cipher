{
  "name": "@qxy/cipher",
  "version": "0.1.1",
  "packageManager": "pnpm@8.14.1",
  "description": "QXY cipher utils",
  "keywords": [
    "qxy",
    "cipher",
    "crypto-js"
  ],
  "license": "MIT",
  "author": {
    "name": "ntnyq",
    "email": "ntnyq13@gmail.com"
  },
  "homepage": "https://github.com/qxy-fe/qxy-cipher#readme",
  "bugs": {
    "url": "https://github.com/qxy-fe/qxy-cipher/issues"
  },
  "repository": "qxy-fe/qxy-cipher",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "unbuild",
    "coverage": "vitest --coverage",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "prepublishOnly": "pnpm run build",
    "release": "run-s release:check release:version release:publish",
    "release:changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "release:check": "run-s typecheck lint",
    "release:publish": "pnpm -r publish",
    "release:version": "bumpp -x \"pnpm release:changelog\" --all",
    "test": "vitest",
    "typecheck": "vitest --typecheck"
  },
  "peerDependencies": {
    "crypto-js": "^4.1.1"
  },
  "dependencies": {
    "@types/crypto-js": "^4.2.1",
    "crypto-js": "^4.2.0"
  },
  "devDependencies": {
    "@ntnyq/eslint-config": "^1.15.0",
    "@ntnyq/prettier-config": "^1.20.1",
    "@types/node": "^20.11.1",
    "@vitest/coverage-v8": "^1.2.0",
    "bumpp": "^9.2.1",
    "conventional-changelog-cli": "^4.1.0",
    "eslint": "^8.56.0",
    "npm-run-all": "^4.1.5",
    "pnpm": "^8.10.0",
    "prettier": "^3.2.2",
    "typescript": "^5.3.3",
    "unbuild": "^2.0.0",
    "vitest": "^1.2.0"
  },
  "prettier": "@ntnyq/prettier-config",
  "publishConfig": {
    "access": "public"
  }
}
